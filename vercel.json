{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "frontend/client/dist",
  "framework": "vite",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "backend/server/server.js": {
      "runtime": "@vercel/node@18"
    }
  }
}
